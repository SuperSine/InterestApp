        <th>
            @Html.DisplayNameFor(model => model.LoanUnit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CapitalAmount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mpr)
        </th>
        <th>
